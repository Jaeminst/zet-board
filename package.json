{
  "name": "zet-board",
  "version": "0.0.4",
  "description": "Desktop app for AWS Tunneling",
  "license": "MIT",
  "author": {
    "name": "Jaemin Lee",
    "email": "jm1225@kakao.com",
    "url": "https://github.com/Jaeminst/zet-board"
  },
  "homepage": "https://github.com/Jaeminst/zet-board#readme",
  "bugs": {
    "url": "https://github.com/Jaeminst/zet-board/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jaeminst/zet-board.git"
  },
  "contributors": [
    {
      "name": "Jaemin Lee",
      "email": "jm1225@kakao.com",
      "url": "https://github.com/Jaeminst/zet-board"
    }
  ],
  "main": "release/dist/main/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -k \"cross-env BROWSER=none npm run next:dev\" \"npm run electron:dev\"",
    "next:dev": "next dev",
    "electron:dev": "npm run build:main && wait-on tcp:3000 && electronmon -r ts-node/register/transpile-only .",
    "lint": "next lint",
    "build": "npm run build:main && next build",
    "build:main": "tsc -p main && npm run build:css",
    "build:css": "npx tailwindcss build --config tailwind.config.main.js -o ./main/tailwind.css --minify"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.529.1",
    "@aws-sdk/client-iam": "^3.521.0",
    "@aws-sdk/client-rds": "^3.525.0",
    "@aws-sdk/client-ssm": "^3.529.1",
    "@aws-sdk/client-sts": "^3.533.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron-serve": "^1.1.0",
    "electron-updater": "^6.1.8",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "lucide-react": "^0.334.0",
    "next": "^13.5.6",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.29",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.1",
    "sonner": "^1.4.0",
    "systeminformation": "^5.22.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.3.3",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "chalk": "^5.3.0",
    "electron": "^26.2.0",
    "electron-builder": "^24.13.3",
    "electronmon": "^2.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  },
  "build": {
    "productName": "ZeT Board",
    "appId": "com.jaeminst.zet-board",
    "files": [
      "release/dist/**/*",
      "node_modules",
      "package.json"
    ],
    "extraResources": [
      "./assets/**"
    ],
    "linux": {
      "category": "System",
      "icon": "assets/icons/",
      "mimeTypes": [
        "text/x-markdown",
        "text/markdown"
      ],
      "target": [
        "rpm",
        "deb"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns",
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false,
      "notarize": false
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": [
        "nsis"
      ]
    }
  },
  "devEngines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "main/**"
    ],
    "logLevel": "quiet"
  }
}
